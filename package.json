{
  "name": "me-node-gibus",
  "version": "1.2.0-SNAPSHOT",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "test": "NODE_ENV=test jest",
    "dev": "NODE_ENV=development nodemon --watch './**/*.ts' --exec node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "start": "NODE_ENV=production node ./lib/index.js",
    "convert-examples": "NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm ./src/converter/convert-examples.ts",
    "convert-production": "NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm ./src/converter/convert-production.ts",
    "test:win": "SET NODE_ENV=test&&jest",
    "dev:win": "SET NODE_ENV=development&&nodemon --watch . -e ts --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts\"",
    "start:win": "SET NODE_ENV=production&&node ./lib/index.js",
    "convert-examples:win": "SET NODE_ENV=development&&node --experimental-specifier-resolution=node --loader ts-node/esm ./src/converter/convert-examples.ts",
    "convert-production:win": "SET NODE_ENV=development&&node --experimental-specifier-resolution=node --loader ts-node/esm ./src/converter/convert-production.ts"
  },
  "author": "Sme.UP",
  "license": "ISC",
  "dependencies": {
    "@sme.up/kokos-sdk-node": "^1.0.0",
   "@langchain/core": "0.3.3",
    "@langchain/openai": "0.3.2", 
    "node-jt400": "^4.5.0"
  },
  "overrides": {
    "class-validator": "0.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
